<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attemp18.DisplayAnimalsMenuController">
   <children>
      <VBox prefHeight="300.0" prefWidth="500.0">
         <children>
            <TableView prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="120.0" text="ID" />
                <TableColumn prefWidth="120.0" text="Breed" />
                  <TableColumn prefWidth="120.0" text="Lifespan" />
                  <TableColumn prefWidth="120.0" text="Price" />
              </columns>
               <VBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" prefWidth="55.0" text="Details" />
                  <Button maxWidth="-Infinity" mnemonicParsing="false" prefWidth="55.0" text="Back" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets right="30.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
